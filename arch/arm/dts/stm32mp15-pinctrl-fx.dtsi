		pins {
			pinmux = <STM32_PINMUX('A', 6, ANALOG)>, /* ADC1 in3 */
				 <STM32_PINMUX('A', 7, ANALOG)>, /* ADC1 in7 */
				 <STM32_PINMUX('B', 1, ANALOG)>, /* ADC1 in5 */
