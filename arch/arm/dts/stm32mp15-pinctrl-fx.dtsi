				 <STM32_PINMUX('F', 11, ANALOG)>, /* ADC1 in2 */
				 <STM32_PINMUX('F', 12, ANALOG)>; /* ADC1 in6 */
		};
	};

	adc12_ain_pins_b: adc12-ain-1 {
		pins {
			pinmux = <STM32_PINMUX('F', 12, ANALOG)>, /* ADC1 in6 */
				 <STM32_PINMUX('F', 13, ANALOG)>; /* ADC2 in2 */
